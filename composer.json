{
    "name": "svea-payments/woocommerce-payment-gateway",
    "description": "Payment module for WooCommerce | Svea Payments Finland",
    "type": "wordpress-plugin",
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.0"
    },
    "license": "Apache-2.0",
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs --standard=WordPress includes/ wc-maksuturva.php -ps"
    }
}
